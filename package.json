{
	"name": "vue-vite-genesis",
	"version": "0.1.0",
	"license": "MIT",
	"author": "zanuka",
	"engines": {
		"node": ">=20",
		"pnpm": ">=8"
	},
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"serve": "vite preview",
		"clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
		"clean:lib": "rimraf node_modules",
		"lint:script": "eslint \"{src/**/*.{ts,vue},cypress/**/*.js}\"",
		"lint:tsc": "vue-tsc --noEmit",
		"lint": "concurrently 'npm run lint:tsc' 'npm run lint:script'",
		"test:unit": "jest --coverage",
		"test": "vitest run src/**/*.spec.ts",
		"test:unit:vitest": "vitest",
		"test:unit:vitest:ui": "vitest --ui",
		"test:config": "TEST_ENV=config vitest __tests__"
	},
	"dependencies": {
		"@headlessui/vue": "^1.7.19",
		"@heroicons/vue": "^2.1.1",
		"@vueuse/core": "^10.9.0",
		"autoprefixer": "^10.4.19",
		"axios": "^1.6.8",
		"insane": "^2.6.2",
		"marked": "^14.0.0",
		"pinia": "^2.2.1",
		"postcss": "~8.4.38",
		"tailwindcss": "^3.4.1",
		"vue": "^3.4.37",
		"vue-router": "^4.4.3"
	},
	"devDependencies": {
		"@pinia/testing": "^0.1.5",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/cypress": "^10.0.2",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@testing-library/vue": "^8.1.0",
		"@testing-library/jest-dom": "^6.6.1",
		"@types/node": "^20.14.11",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-vue": "^5.1.2",
		"@vitest/coverage-v8": "^2.0.5",
		"@vitest/ui": "^2.1.2",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.6",
		"concurrently": "^8.2.2",
		"cypress": "^13.13.2",
		"eslint": "^8.57.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-vue": "^9.19.2",
		"happy-dom": "^14.12.3",
		"jsdom": "^22.1.0",
		"msw": "^2.4.8",
		"prettier": "^3.3.3",
		"resize-observer-polyfill": "^1.5.1",
		"rimraf": "^6.0.1",
		"rollup-plugin-visualizer": "^5.14.0",
		"swagger-typescript-api": "^13.0.16",
		"typescript": "~5.5.4",
		"vite": "^5.4.0",
		"vite-svg-loader": "^4.0.0",
		"vitest": "^2.1.9",
		"vitest-dom": "^0.1.1",
		"vue-tsc": "^2.0.29"
	}
}
